{
    "name": "fyyb/express",
    "description": "PHP micro framework that helps you quickly write simple yet powerful web applications and APIs.",
    "version": "2.0.4",
    "keywords": [
        "Fyyb",
        "framework",
        "micro",
        "api",
        "router"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joao Netto",
            "email": "jnetto@fyyb.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Fyyb\\": "./express/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "scripts": {
        "check-style": "php-cs-fixer fix --diff --verbose --dry-run .",
        "fix-style": "php-cs-fixer fix ."
    }
}
